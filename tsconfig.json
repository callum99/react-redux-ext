{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "target": "esnext",
    "lib": ["es6", "es2015", "dom"],
    "module": "esnext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": true,
    "outDir": "dist",
    "rootDir": "src",
    "jsx": "react-jsx"
  },
  "exclude": [
    "node_modules"
  ],
  "files": [
    "src/index.tsx"
  ]
}
